{
    "name": "rutafem-app-backend",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "test:errors": "node scripts/test-errors.js",
        "test:smoke": "cd .. && DISABLE_RATE_LIMIT=true docker-compose run --rm k6 run --out experimental-prometheus-rw /scripts/smoke-test.js",
        "test:stress": "cd .. && DISABLE_RATE_LIMIT=true docker-compose run --rm k6 run --out experimental-prometheus-rw /scripts/stress-test.js",
        "test:smoke:local": "cd .. && DISABLE_RATE_LIMIT=true docker-compose run --rm k6 run /scripts/smoke-test.js",
        "test:stress:local": "cd .. && DISABLE_RATE_LIMIT=true docker-compose run --rm k6 run /scripts/stress-test.js",
        "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./app.ts | pino-pretty -c -t 'SYS:standard' -i pid,hostname",
        "dev:docker": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./app.ts",
        "build": "tsc",
        "start": "node dist/app.js"
    },
    "keywords": [],
    "author": "rutafem",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@prisma/client": "^6.17.1",
        "better-auth": "^1.3.27",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "ioredis": "^5.8.2",
        "pino": "^10.0.0",
        "pino-pretty": "^13.0.0",
        "prom-client": "^15.1.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/ioredis": "^4.28.10",
        "@types/node": "^24.7.2",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "prisma": "^6.17.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    }
}
